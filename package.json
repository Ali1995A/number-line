{
  "name": "number-line",
  "version": "2.1.5",
  "description": "A customizable & virtually inifinite number line that can be used by any renderer",
  "source": "lib/number-line.ts",
  "main": "dist/number-line.js",
  "types": "dist/number-line.d.ts",
  "targets": {
    "main": {
      "source": "lib/number-line.ts",
      "distDir": "dist",
      "outputFormat": "commonjs",
      "isLibrary": true
    },
    "types": {
      "source": "lib/number-line.ts",
      "distDir": "dist",
      "isLibrary": true
    },
    "site": {
      "source": "src/index.html",
      "distDir": "site-dist",
      "publicUrl": "/",
      "context": "browser"
    }
  },
  "scripts": {
    "test": "jest",
    "dev": "parcel src/index.html",
    "build": "parcel build --target main --target types",
    "build:site": "parcel build --target site",
    "vercel-build": "npm run build:site"
  },
  "keywords": [
    "number line",
    "maths",
    "graphics",
    "render",
    "equation",
    "visualization",
    "viz",
    "infographics",
    "graph"
  ],
  "author": "nikhilnxvverma1",
  "license": "MIT",
  "dependencies": {
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@parcel/babel-preset-env": "^2.12.0",
    "@parcel/packager-ts": "^2.6.2",
    "@parcel/transformer-sass": "^2.6.2",
    "@parcel/transformer-typescript-types": "^2.6.2",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "parcel": "^2.6.2",
    "postcss": "^8.4.36",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.7.4"
  }
}
